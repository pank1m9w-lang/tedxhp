# EmailJS Configuration
# Get these values from https://dashboard.emailjs.com/

# Public Key (Account > General)
PUBLIC_EMAILJS_PUBLIC_KEY=your_public_key_here

# Service ID (Email Services)
PUBLIC_EMAILJS_SERVICE_ID=your_service_id_here

# Template ID for main notification email (Email Templates)
PUBLIC_EMAILJS_TEMPLATE_ID=sponsor_application

# Template ID for auto-reply email (Optional)
PUBLIC_EMAILJS_REPLY_TEMPLATE_ID=sponsor_application_reply
